<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:priv-android="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" android:paddingMode="stack">
    <item android:gravity="center" android:id="@android:id/background">
        <inset>
            <shape>
                <size android:height="2dp"/>
                <corners android:radius="2dp"/>
                <solid android:color="@color/brightness_progress_normal_color"/>
            </shape>
        </inset>
    </item>
    <item android:gravity="center" android:id="@android:id/progress">
        <clip>
            <inset>
                <shape>
                    <size android:height="2dp"/>
                    <corners android:radius="2dp"/>
                    <solid android:color="?android:^attr-private/colorListDivider"/>
                </shape>
            </inset>
        </clip>
    </item>
</layer-list>
