<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="@color/settingslib_ripple_color">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <solid android:color="?android:bool/config_allowTheaterModeWakeFromWindowLayout"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="?android:bool/config_allowTheaterModeWakeFromGesture"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
</ripple>
