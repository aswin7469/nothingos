<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" xmlns:settings="http://schemas.android.com/apk/res-auto" android:title="@string/about_settings" android:key="my_device_info_pref_screen">
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/settings_entity_header" android:selectable="false" android:key="my_device_info_header" android:order="0" settings:isPreferenceVisible="false"/>
    <PreferenceCategory android:selectable="false" android:key="nt_my_device_info_basic_pref_category">
        <com.nt.settings.about.NtAboutPhoneBasicInfoPreference android:selectable="false" android:key="nt_my_device_info_basic" android:order="1" settings:controller="com.nt.settings.about.NtDeviceNamePreferenceController"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/nt_hardware" android:selectable="false" android:key="nt_my_device_info_hardware">
        <Preference android:title="@string/nt_processor" android:selectable="false" android:key="nt_hardware_processor" android:summary="@string/nt_processor_summary" android:order="2" settings:enableCopying="true"/>
        <com.nt.settings.about.NtAboutPhoneRamBatteryPreference android:selectable="false" android:key="nt_hardware_ram_battery" android:order="3" settings:enableCopying="true"/>
        <com.nt.settings.about.NtAboutPhoneModelSNPreference android:selectable="false" android:key="nt_hardware_model_serial" android:order="4" settings:enableCopying="true"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/my_device_info_device_identifiers_category_title">
        <com.nt.settings.about.NtAboutPhoneSimPreference android:selectable="false" android:key="nt_device_sims" android:order="5" settings:enableCopying="true"/>
        <Preference android:icon="@drawable/ic_chevron_right_24dp" android:title="@string/previous_connected_see_all" android:key="nt_device_see_all" android:order="6" settings:controller="com.nt.settings.about.NtDeviceSeeAllPreferenceController" settings:searchable="false"/>
        <com.nt.settings.about.NtAboutPhoneImeiPreference android:selectable="false" android:key="nt_device_imei_info" android:order="7" settings:enableCopying="true"/>
        <Preference android:title="@string/wifi_ip_address" android:selectable="false" android:key="wifi_ip_address" android:summary="@string/summary_placeholder" android:order="44" settings:enableCopying="true"/>
        <Preference android:title="@string/status_wifi_mac_address" android:key="saved_accesspoints_wifi_mac_address" android:summary="@string/view_saved_network" android:order="45" android:fragment="com.android.settings.wifi.savedaccesspoints2.SavedAccessPointsWifiSettings2" settings:enableCopying="false" settings:isPreferenceVisible="false"/>
        <Preference android:title="@string/status_device_wifi_mac_address" android:selectable="false" android:key="wifi_mac_address" android:summary="@string/summary_placeholder" android:order="46" settings:enableCopying="true"/>
        <Preference android:title="@string/status_bt_address" android:selectable="false" android:key="bt_address" android:summary="@string/summary_placeholder" android:order="47" settings:enableCopying="true"/>
        <Preference android:title="@string/status_up_time" android:selectable="false" android:key="up_time" android:summary="@string/summary_placeholder" android:order="48"/>
        <Preference android:title="@string/firmware_version" android:key="firmware_version" android:summary="@string/summary_placeholder" android:order="49" android:fragment="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionSettings" settings:controller="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionPreferenceController"/>
        <Preference android:title="@string/manual" android:key="manual" android:order="50">
            <intent android:action="android.settings.SHOW_MANUAL"/>
        </Preference>
        <Preference android:title="@string/device_feedback" android:key="device_feedback" android:order="51" settings:keywords="@string/keywords_device_feedback"/>
        <Preference android:title="@string/fcc_equipment_id" android:key="fcc_equipment_id" android:summary="@string/summary_placeholder" android:order="52"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/my_device_info_legal_category_title">
        <Preference android:title="@string/legal_information" android:key="legal_container" android:order="15" android:fragment="com.android.settings.LegalSettings"/>
        <Preference android:title="@string/regulatory_labels" android:key="regulatory_info" android:order="16">
            <intent android:action="android.settings.SHOW_REGULATORY_INFO"/>
        </Preference>
        <Preference android:title="@string/safety_and_regulatory_info" android:key="safety_info" android:order="17">
            <intent android:action="android.settings.SHOW_SAFETY_AND_REGULATORY_INFO"/>
        </Preference>
    </PreferenceCategory>
    <Preference android:title="@string/software_version" android:selectable="false" android:key="software_version" android:summary="@string/summary_placeholder" android:order="44" settings:enableCopying="true"/>
    <Preference android:title="@string/ram_total_size" android:selectable="false" android:key="key_storage_total_size" android:summary="@string/summary_placeholder" android:order="45" settings:allowDividerAbove="true" settings:enableCopying="true"/>
</PreferenceScreen>
