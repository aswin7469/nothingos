<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:androidprv="http://schemas.android.com/apk/prv/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:title="@string/about_settings" android:key="my_device_info_pref_screen">
    <com.android.settingslib.widget.LayoutPreference android:layout="@layout/settings_entity_header" android:selectable="false" android:key="my_device_info_header" android:order="0" app:isPreferenceVisible="false"/>
    <PreferenceCategory android:selectable="false" android:key="device_info_basic_category">
        <com.nothing.settings.deviceinfo.aboutphone.DeviceBasicInfoPreference android:selectable="false" android:key="device_info_basic" android:order="1" app:controller="com.nothing.settings.deviceinfo.DeviceNamePreferenceController"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/nt_device_details" android:selectable="false" android:key="nt_my_device_info_hardware">
        <Preference android:title="@string/nt_processor" android:selectable="false" android:key="nt_hardware_processor" android:summary="@string/nt_processor_summary" android:order="2" app:enableCopying="true"/>
        <com.nothing.settings.deviceinfo.aboutphone.DeviceRamBatteryPreference android:selectable="false" android:key="device_info_hardware_ram_battery" android:order="3" app:enableCopying="true"/>
        <com.nothing.settings.deviceinfo.aboutphone.DeviceModelSNPreference android:selectable="false" android:key="device_info_hardware_model_serial" android:order="4" app:enableCopying="true"/>
        <Preference android:title="@string/firmware_version" android:key="firmware_version" android:summary="@string/summary_placeholder" android:order="5" android:fragment="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionSettings" app:controller="com.android.settings.deviceinfo.firmwareversion.FirmwareVersionPreferenceController"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/my_device_info_device_identifiers_category_title">
        <com.nothing.settings.deviceinfo.aboutphone.DeviceSimPreference android:selectable="false" android:key="nt_device_sims" android:order="10" app:enableCopying="true"/>
        <Preference android:icon="@drawable/ic_arrow_right_24dp" android:title="@string/previous_connected_see_all" android:key="nt_device_see_all" android:order="11" app:controller="com.nothing.settings.deviceinfo.aboutphone.DeviceSeeAllPreferenceController" app:searchable="false"/>
        <com.nothing.settings.deviceinfo.aboutphone.DeviceImeiPreference android:selectable="false" android:key="nt_device_imei_info" android:order="12" app:controller="com.nothing.settings.deviceinfo.aboutphone.ImeiInfoPreferenceController" app:enableCopying="true" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/wifi_ip_address" android:selectable="false" android:key="wifi_ip_address" android:summary="@string/summary_placeholder" android:order="13" app:enableCopying="true" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/status_wifi_mac_address" android:key="saved_accesspoints_wifi_mac_address" android:summary="@string/view_saved_network" android:order="14" android:fragment="com.android.settings.wifi.savedaccesspoints2.SavedAccessPointsWifiSettings2" app:enableCopying="false" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/status_device_wifi_mac_address" android:selectable="false" android:key="wifi_mac_address" android:summary="@string/summary_placeholder" android:order="15" app:enableCopying="true" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/status_bt_address" android:selectable="false" android:key="bt_address" android:summary="@string/summary_placeholder" android:order="16" app:enableCopying="true" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/status_up_time" android:selectable="false" android:key="up_time" android:summary="@string/summary_placeholder" android:order="17" app:isPreferenceVisible="false"/>
        <Preference android:title="@string/manual" android:key="manual" android:order="18">
            <intent android:action="android.settings.SHOW_MANUAL"/>
        </Preference>
        <Preference android:title="@string/device_feedback" android:key="device_feedback" android:order="19" app:keywords="@string/keywords_device_feedback"/>
        <Preference android:title="@string/fcc_equipment_id" android:key="fcc_equipment_id" android:summary="@string/summary_placeholder" android:order="20"/>
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/my_device_info_legal_category_title">
        <Preference android:title="@string/legal_information" android:key="legal_container" android:order="30" android:fragment="com.android.settings.LegalSettings"/>
        <Preference android:title="@string/nt_regulatory_labels" android:key="regulatory_info" android:order="31">
            <intent android:action="android.settings.SHOW_REGULATORY_INFO"/>
        </Preference>
        <Preference android:title="@string/safety_and_regulatory_info" android:key="safety_info" android:order="32">
            <intent android:action="android.settings.SHOW_SAFETY_AND_REGULATORY_INFO"/>
        </Preference>
    </PreferenceCategory>
    <Preference android:title="@string/software_version" android:selectable="false" android:key="software_version" android:summary="@string/summary_placeholder" android:order="40" app:enableCopying="true"/>
    <Preference android:title="@string/ram_total_size" android:selectable="false" android:key="key_storage_total_size" android:summary="@string/summary_placeholder" android:order="41" app:allowDividerAbove="true" app:enableCopying="true"/>
</PreferenceScreen>
